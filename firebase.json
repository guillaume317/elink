{
  "firebase": "elink",
  "public": ".",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "headers": [ {
        "source" : "**/*.*)",
        "headers" : [ {
            "key" : "Access-Control-Allow-Origin",
            "value" : "*"
          } ]
        }, {
        "source" : "**/*.@(js|css|jpg|jpeg|gif|png)",
        "headers" : [ {
            "key" : "Cache-Control",
            "value" : "max-age=7200"
          } ]
  }]

}